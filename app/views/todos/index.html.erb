<div class="text-center" style="padding:15px; margin: 10px 0;">
	<h2>全ユーザーのTODO</h2>
</div>

<div class="col-xs-8">
	<ul class="media-list">
    <% @todos.each do |todo| %>
      <%= render 'todos/todolist', todo: todo, user: todo.user %>
    <% end %>
	</ul>

</div><!--/.l-main-->

<div class="l-pagination">
<%= paginate @todos %>
</div>
